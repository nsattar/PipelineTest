	<xs:complexType name="AdjustCustomerPointsRequest">
		<xs:annotation>
			<xs:documentation>调整Identity积分的请求消息</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PointList">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="Point">
							<xs:complexType>
								<xs:sequence>
									<xs:element minOccurs="0" name="PointType">
										<xs:annotation>
											<xs:documentation>积分类型的Alias。如果Identity存在多个积分类型的积分，则必须输入此参数。</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="128"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="PointID" type="xs:long">
										<xs:annotation>
											<xs:documentation>如果Identity存在多个同一类型的积分，则必须输入此参数</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Amount" type="xs:long">
										<xs:annotation>
											<xs:documentation>积分调整数量。调增为正数，调减为负数。</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element minOccurs="0" name="ChangeReason">
										<xs:annotation>
											<xs:documentation>调整原因说明</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="512"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="Remark">
										<xs:annotation>
											<xs:documentation>备注</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="1000"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element minOccurs="0" name="ExpiredDate">
										<xs:annotation>
											<xs:documentation>积分的失效日期，格式YYYYMMDD</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="8"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AdjustmentCategory" type="xs:string">
				<xs:annotation>
					<xs:documentation>积分变动类型： 1:调整积分 2:兑换积分</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
