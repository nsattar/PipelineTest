	<xs:complexType name="BulkTaskRequest">
		<xs:annotation>
			<xs:documentation>批量任务的请求</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FilePath">
				<xs:annotation>
					<xs:documentation>Indicates the request file name</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TaskType">
				<xs:annotation>
					<xs:documentation>Indicates the task type for the bulk operation.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TaskName">
				<xs:annotation>
					<xs:documentation>Indicates the task name for the bulk operation.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TaskDesc">
				<xs:annotation>
					<xs:documentation>Indicates the task description.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="ServiceCode">
				<xs:annotation>
					<xs:documentation>Indicates a unique service code (service alias) defined by the system administrator.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="ReasonType">
				<xs:annotation>
					<xs:documentation>Indicates the reason type for a service or transaction. This field can be set to the reason type alias.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="128"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ScheduleDay" type="common:Date">
				<xs:annotation>
					<xs:documentation>Indicates the date when the bulk task should be executed in Mobile Money system. The format for this parameter must be YYYYMMDD.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ScheduleHour" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Indicates the daily hour when the bulk task should be executed in Mobile Money system. The value range for this parameter is from 0 to 23.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="NotificationType" type="common:IDType">
				<xs:annotation>
					<xs:documentation>Internal ID of Notification Type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="CommonText">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="Parameters">
				<xs:annotation>
					<xs:documentation>It is used to carry specific parameters for specific bulk task.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
